{
    "$schema": "http://json.schemastore.org/template",
    "author": "MSBuild.Sdk.SqlProj",
    "classifications": [ "Database", "SqlServer", "Table" ],
    "identity": "MSBuild.Sdk.SqlProj.Table",
    "name": "Database table",
    "shortName": "table",
    "tags": {
      "language": "SQL",
      "type": "item"
    },
    "sourceName": "Table1",
    "symbols": {
      "schemaName": {
        "type": "parameter",
        "defaultValue": "dbo",
        "replaces": "#{SchemaName}",
        "description": "Name of the schema to place the table in. Defaults to 'dbo'."
      }
    }
  }